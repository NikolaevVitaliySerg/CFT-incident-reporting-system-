class S16_TYPETASK;

@name('Добавить')
@tag('CREATED')
constructor NEW_AUTO is

NEW_AUTO (
	@name('Название')
	P_NAME in [STRING_50]
) 
return ref [S16_TYPETASK] is

validate is
begin
	if p_message = 'DEFAULT' then
		P_NAME := this.[NAME];
	end if;
end;

execute is
begin
	-- Установка значения реквизита "Название"
	[NAME] := P_NAME;
end;
end;
end;
